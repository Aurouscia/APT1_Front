
<cover-view class="tab-bar" capture-bind:tap="">
  <cover-view class="tab-bar-border"></cover-view>
  <cover-view wx:for="{{listTab}}" wx:key="index" class="tab-bar-item" data-path="{{item.pagePath}}" data-index="{{index}}" capture-bind:tap="switchTab">
    <cover-image class="cover-image" src="{{item.iconPath}}"></cover-image>
    <cover-view style="color: {{color}}">{{item.text}}</cover-view>
  </cover-view>
</cover-view>
